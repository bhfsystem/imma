#!/usr/bin/env bash

export CONTEXT_ORG="${0##*/}"

function main {
  local shome="${_imma_home:-"$(cd -P -- "${BASH_SOURCE%/*}/.." && pwd -P)"}"
  source "$shome/script/profile"

  exec aws exec "$@"
}

if [[ -z "${CONTEXT_REGION:-}" ]]; then
  source sub-chain aws us-west-1 "$BASH_SOURCE" "$@"
else
  source sub-chain "$BASH_SOURCE" "$@"
fi
