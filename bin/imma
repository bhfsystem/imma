#!/usr/bin/env bash

export CONTEXT_ORG="${0##*/}"
source aws-us-west-1 ''

function main {
  local shome="$(cd -P -- "${BASH_SOURCE%/*}/.." && pwd -P)"
  source "$shome/script/profile"
  source normalize

  export TF_VAR_aws_account_id="${TF_VAR_aws_account_id:-"$(aws iam get-user | jq -r '.User.Arn | split(":")[4]')"}"

  exec "$@"
}

source sub-chain "$BASH_SOURCE" "$@"
