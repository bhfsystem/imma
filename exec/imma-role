#!/usr/bin/env bash

function main {
  local shome="$(cd -P -- "${BASH_SOURCE%/*}/.." && pwd -P)"
  source "$shome/script/profile"
  source normalize

  local nm_role="$1"; shift
  local the_code="$1"; shift

  exec assume-aws-role "${CONTEXT_ORG}-${nm_role#${CONTEXT_ORG}-}" "$the_code" env "$@"
}

source sub "$BASH_SOURCE" "$@"
